{
  "name": "biliLive-tools",
  "version": "0.14.0",
  "description": "B站直播录制处理工具",
  "main": "./out/main/index.js",
  "author": "renmu123",
  "license": "GPL-3.0",
  "homepage": "https://github.com/renmu123/biliLive-tools",
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && electron-vite dev",
    "devMain": "chcp 65001 && electron-vite dev --watch",
    "start": "electron-vite preview",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "pnpm run build && electron-builder --win --config",
    "build:win-no-ffmpeg": "pnpm run build && electron-builder --win --config electron-builder-no-ffmpeg.yml",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "test": "vitest"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@napi-rs/canvas": "^0.1.51",
    "@renmu/bili-api": "^1.3.1",
    "ass-compiler": "^0.1.11",
    "axios": "^1.6.8",
    "electron-log": "^5.1.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "express": "^4.19.2",
    "fast-xml-parser": "^4.3.6",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "^11.2.0",
    "lodash-es": "^4.17.21",
    "nodemailer": "^6.9.13",
    "ntsuspend": "^1.0.2",
    "pinia": "^2.1.7",
    "semver": "^7.6.0",
    "sortablejs": "^1.15.2",
    "vuedraggable": "^4.1.0",
    "@biliLive-tools/shared": "workspace:*",
    "@biliLive-tools/types": "workspace:*"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "20.12.2",
    "@types/nodemailer": "^6.4.14",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^10.9.0",
    "electron": "30.0.1",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.1.0",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.25.0",
    "less": "^4.2.0",
    "naive-ui": "^2.38.1",
    "qrcode-vue3": "^1.6.8",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.13"
  }
}
